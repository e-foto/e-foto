!!!139384.cpp!!!	reloadProject() : bool

	nextModule = 0;
	if (project != NULL)
	{
		if (interiorOrientation != NULL)
		{
			stopIO(interiorOrientation->getId());
		}
		if (spatialRessection != NULL)
		{
			stopSR(spatialRessection->getId());
		}
		return project->reload();
	}
	else
	{
		return false;
	}
