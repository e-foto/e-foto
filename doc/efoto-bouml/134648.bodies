		else if (xmlPoint.attribute("type").compare("control") == 0)
		{
			PhotogrammetricPoint* myPhotogrammetric = (PhotogrammetricPoint*) myPoint;
			delete(myPhotogrammetric);
		}
		points.erase(points.begin() + i);
	}
